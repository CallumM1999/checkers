<div class='modal' id='<%= id %>'>
    <div class="modal__background"/>
    <div class="modal__body">

        <h1 class="modal__title">Game Ended</h1>
        <p class="modal__message">{ this.state.winMessage }</p>
        <div class="modal__options">
            <button class="button modal__button">Rematch</button>
            <button class="button modal__button">Home</button>
        </div>

    </div>
</div>